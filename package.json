{
  "name": "klleon-sdk",
  "packageManager": "yarn@4.3.1",
  "version": "0.0.1",
  "workspaces": {
    "packages": [
      "apps/*",
      "packages/*"
    ]
  },
  "scripts": {
    "js:dev": "yarn workspace js-sdk dev",
    "react:dev": "yarn workspace @klleon/react-sdk dev",
    "react:build": "yarn workspace @klleon/react-sdk build:prod",
    "lint": "eslint ./apps --ext .ts,.tsx,.js,.jsx --fix"
  },
  "devDependencies": {
    "debug": "^4.3.5",
    "eslint": "^8.57.0",
    "eslint-plugin-lit": "^1.14.0",
    "prettier": "^3.3.2",
    "supports-color": "^9.4.0",
    "typescript": "^5.5.3"
  }
}
